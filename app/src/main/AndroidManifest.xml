<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learnmandarin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Animals"
            android:label="Animals"/>
        <activity android:name=".Places"
            android:label="Places"/>
        <activity android:name=".Greetings"
            android:label="Greetings"/>
        <activity android:name=".Transportation"
            android:label="Transportation"/>
        <activity android:name=".Families"
            android:label="Families"/>
        <activity android:name=".Basics"
            android:label="Basics"/>
        <activity android:name=".NumbersActivity"
            android:label="Numbers"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>